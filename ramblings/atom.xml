---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Sean's Ramblings</title>
	<link href="http://seanmythen.net/ramblings/atom.xml" rel="self"/>
	<link href="http://seanmythen.net/ramblings"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://seanmythen.net/ramblings</id>
	<author>
		<name>Sean Mythen</name>
		<email>mythen.sean@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://seanmythen.net{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://seanmythen.net{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
